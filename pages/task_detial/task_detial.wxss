.top-view {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: flex-start;
  width: 100%;
}

.top-view-right {
  font-size: 20rpx;
  color: #000000;
}

.top-view-left {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: flex-start;
}

.top-view-left image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 10rpx;
}

.top-view-left view {
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  margin-left: 20rpx;
  flex-direction: column;
  align-items: flex-start;
  height: 120rpx;
}

.nine-view {
  display: -webkit-flex;
  justify-content: flex-start;
  align-items: center;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 100%;
}

.nine-item-view {
  display: -webkit-flex;
  display: flex;
  width: 32%;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  margin: 5rpx;
}

.nine-image {
  width: 100%;
  height: 240rpx;
}


